{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "wat",
      "camelcase": "Wat",
      "title": "WebAssembly Text Format",
      "scope": "source.wat",
      "file-types": ["wat"],
      "injection-regex": "^wat$",
      "class-name": "TreeSitterWat",
      "highlights": "queries/highlights.scm",
      "tags": "queries/tags.scm"
    }
  ],
  "metadata": {
    "version": "0.1.0",
    "license": "MIT",
    "description": "Wat grammar for tree-sitter",
    "authors": [
      {
        "name": "Pig Fang",
        "email": "g-plane@hotmail.com",
        "url": "https://github.com/g-plane"
      }
    ],
    "links": {
      "repository": "https://github.com/g-plane/tree-sitter-wat",
      "funding": "https://github.com/sponsors/g-plane"
    }
  },
  "bindings": {
    "c": true,
    "go": false,
    "java": false,
    "node": true,
    "python": true,
    "rust": true,
    "swift": false,
    "zig": false
  }
}
